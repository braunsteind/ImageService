<Window x:Class="ImageServiceGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageServiceGUI.Views"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="400" MinWidth="400" SizeToContent="WidthAndHeight">
    <Grid>
        <TabControl x:Name="tabControl" Height="Auto" Width="Auto">
            <TabItem Header="Settings">
                <local:SettingsView/>
            </TabItem>
            <TabItem Header="Logs">
                <Grid Background="#FFE535E7"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
